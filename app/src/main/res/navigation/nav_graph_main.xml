    <fragment
        android:id="@+id/speakerFragment"
        android:name="com.litbig.engapp.testcase.SpeakerFragment"
        android:label="SpeakerFragment" />
    <fragment
        android:id="@+id/wiFiFragment"
        android:name="com.litbig.engapp.testcase.WiFiFragment"
        android:label="WiFiFragment" />
    <action
        android:id="@+id/action_global_mainFragment"
        app:destination="@id/mainFragment" />
    <fragment
        android:id="@+id/BTFragment"
        android:name="com.litbig.engapp.testcase.BTFragment"
        android:label="fragment_bt"
