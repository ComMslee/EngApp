<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_main.xml"
    app:startDestination="@id/mainFragment">


    <fragment
        android:id="@+id/mainFragment"
        android:name="com.litbig.engapp.MainFragment"
        android:label="fragment_main"
        tools:layout="@layout/fragment_main">
        <action
            android:id="@+id/action_mainFragment_to_micFragment"
            app:destination="@id/micFragment" />
        <action
            android:id="@+id/action_mainFragment_to_dipSwitchFragment"
            app:destination="@id/dipSwitchFragment" />
        <action
            android:id="@+id/action_mainFragment_to_gpsFragment"
            app:destination="@id/gpsFragment" />
        <action
            android:id="@+id/action_mainFragment_to_speakerFragment"
            app:destination="@id/speakerFragment" />
        <action
            android:id="@+id/action_mainFragment_to_wiFiFragment"
            app:destination="@id/wiFiFragment" />
        <action
            android:id="@+id/action_mainFragment_to_diskFragment"
            app:destination="@id/diskFragment" />
        <action
            android:id="@+id/action_mainFragment_to_BTFragment"
            app:destination="@id/BTFragment" />
        <action
            android:id="@+id/action_mainFragment_to_LVDSFragment"
            app:destination="@id/LVDSFragment" />
    </fragment>
    <fragment
        android:id="@+id/dipSwitchFragment"
        android:name="com.litbig.engapp.testcase.DipSwitchFragment"
