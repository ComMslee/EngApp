<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/black"
        tools:context=".testcase.DipSwitchFragment">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/result"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <androidx.appcompat.widget.LinearLayoutCompat
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="20dp"
                android:orientation="horizontal"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="1"
                        android:textColor="@color/btn_text_normal"
                        android:textSize="@dimen/fac_text_normal_m" />

                    <TextView
                        android:id="@+id/tvOnDIP01"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="@drawable/bg_fac_text"
                        android:gravity="center"
                        android:padding="10dp"
                        android:text="ON"
                        android:textColor="@color/btn_text_normal"
                        android:textSize="@dimen/fac_text_normal_m" />

                    <TextView
                        android:id="@+id/tvOffDIP01"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="@drawable/bg_fac_text"
                        android:gravity="center"
                        android:padding="10dp"
                        android:text="OFF"
                        android:textColor="@color/btn_text_normal"
                        android:textSize="@dimen/fac_text_normal_m" />

                </androidx.appcompat.widget.LinearLayoutCompat>

                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="2"
                        android:textColor="@color/btn_text_normal"
                        android:textSize="@dimen/fac_text_normal_m" />

                    <TextView
                        android:id="@+id/tvOnDIP02"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="@drawable/bg_fac_text"
                        android:gravity="center"
                        android:padding="10dp"
                        android:text="ON"
                        android:textColor="@color/btn_text_normal"
                        android:textSize="@dimen/fac_text_normal_m" />

                    <TextView
                        android:id="@+id/tvOffDIP02"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="@drawable/bg_fac_text"
                        android:gravity="center"
                        android:padding="10dp"
                        android:text="OFF"
                        android:textColor="@color/btn_text_normal"
                        android:textSize="@dimen/fac_text_normal_m" />

                </androidx.appcompat.widget.LinearLayoutCompat>

                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="3"
                        android:textColor="@color/btn_text_normal"
                        android:textSize="@dimen/fac_text_normal_m" />

                    <TextView
                        android:id="@+id/tvOnDIP03"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="@drawable/bg_fac_text"
                        android:gravity="center"
                        android:padding="10dp"
                        android:text="ON"
                        android:textColor="@color/btn_text_normal"
                        android:textSize="@dimen/fac_text_normal_m" />

                    <TextView
                        android:id="@+id/tvOffDIP03"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/bg_fac_text"
                        android:gravity="center"
                        android:padding="10dp"
                        android:text="OFF"
                        android:textColor="@color/btn_text_normal"
                        android:textSize="@dimen/fac_text_normal_m" />

                </androidx.appcompat.widget.LinearLayoutCompat>

                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="4"
                        android:textColor="@color/btn_text_normal"
                        android:textSize="@dimen/fac_text_normal_m" />

                    <TextView
                        android:id="@+id/tvOnDIP04"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="@drawable/bg_fac_text"
                        android:gravity="center"
                        android:padding="10dp"
                        android:text="ON"
                        android:textColor="@color/btn_text_normal"
                        android:textSize="@dimen/fac_text_normal_m" />

                    <TextView
                        android:id="@+id/tvOffDIP04"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="@drawable/bg_fac_text"
                        android:gravity="center"
                        android:padding="10dp"
                        android:text="OFF"
                        android:textColor="@color/btn_text_normal"
                        android:textSize="@dimen/fac_text_normal_m" />

                </androidx.appcompat.widget.LinearLayoutCompat>
            </androidx.appcompat.widget.LinearLayoutCompat>
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.fragment.app.FragmentContainerView
            android:id="@+id/result"
            android:name="com.litbig.engapp.testcase.ResultFragment"
            android:layout_width="@dimen/selectArea"
            android:layout_height="0dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>